{
  "name": "twack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.dev -- next dev --turbopack",
    "devw": "dotenv -e .env.dev -- next dev --webpack",
    "build": "dotenv -e .env.dev -- next build --turbopack",
    "start": "dotenv -e .env.dev -- next start",
    "lint": "eslint",
    "studio": "dotenv -e .env.dev -- pnpm dlx prisma studio",
    "prisma:migrate": "dotenv -e .env.dev -- pnpm dlx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.dev -- pnpm dlx prisma generate --no-engine"
  },
  "browserslist": [
    "chrome 135",
    "edge 135"
  ],
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.18.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-next-experimental": "^5.90.2",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "client-only": "^0.0.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jotai": "^2.15.0",
    "jotai-devtools": "^0.13.0",
    "next": "16.0.1",
    "next-auth": "5.0.0-beta.30",
    "prisma": "^6.18.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "sass": "^1.93.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.4",
    "sass-embedded": "^1.93.2",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "^5.9.3"
  }
}

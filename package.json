    "prisma": "^7.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "^6.1.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"

{
  "name": "twack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev --turbopack",
    "devw": "dotenv -e .env -- next dev --webpack",
    "build": "dotenv -e .env -- next build --turbopack",
    "start": "dotenv -e .env -- next start",
    "lint": "eslint",
    "studio": "dotenv -e .env -- pnpm dlx prisma studio",
    "prisma:migrate": "dotenv -e .env -- pnpm dlx prisma migrate dev",
    "prisma:generate": "dotenv -e .env -- pnpm dlx prisma generate --no-engine"
  },
  "browserslist": [
    "chrome 135",
    "edge 135"
  ],
  "dependencies": {
    "@auth/core": "^0.34.3",
